_stop-video-screensaver()
{
    local cur prev opts
    COMPREPLY=()
    cur="${COMP_WORDS[COMP_CWORD]}"
    prev="${COMP_WORDS[COMP_CWORD-1]}"

    if [[ ${cur} == start* ]] ; then
        COMPREPLY=( $(compgen -W ${cur}) )
        return 0
    fi
}
complete -M _stop-video-screensaver stop-video-screensaver
